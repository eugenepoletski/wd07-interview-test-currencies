{
  "name": "interview-test-currencies",
  "version": "0.0.1",
  "description": "React-redux boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "server": "node server.js",
    "start": "webpack-dev-server --color --config webpack.dev.js",
    "test": "jest"
  },
  "author": "e.pol",
  "license": "ISC",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "./src/shim.js",
      "./src/setupTests.js"
    ],
    "verbose": true
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "express": "^4.18.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "react-test-renderer": "^16.0.0",
    "redux-devtools": "^3.4.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3",
    "xml-js": "^1.5.1"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-developer-poletski/boilerplate-react-redux-jest.git"
  },
  "bugs": {
    "url": "https://github.com/web-developer-poletski/boilerplate-react-redux-jest/issues"
  },
  "homepage": "https://github.com/web-developer-poletski/boilerplate-react-redux-jest#readme",
  "keywords": [
    "interview",
    "currencies"
  ]
}
